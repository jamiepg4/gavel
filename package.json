{
  "name": "GAVEL",
  "version": "1.0.0",
  "description": "Auction tool",
  "repository": {
    "type": "git",
    "url": "git://github.com/fusioneng/gavel.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
