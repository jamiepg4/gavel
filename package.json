    "gulp": "*",
    "gulp-changed": "*",
    "gulp-clean": "*",
    "gulp-imagemin": "*",
    "gulp-jsonminify": "*",
    "gulp-minify-css": "*",
    "gulp-notify": "*",
    "gulp-rev-all": "*",
    "gulp-ruby-sass": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "pretty-hrtime": "*",
    "require-dir": "*",
    "rimraf": "*",
    "vinyl-source-stream": "*",
    "watchify": "*"
  },
  "dependencies": {
    "backbone": "*",
    "jquery": "*",
    "underscore": "*"
  }
